-- helper function to create UI elements succinctly
local function Create(className, props, children)
    local obj = Instance.new(className)
    for k, v in pairs(props) do
        obj[k] = v
    end
    if children then
        for _, child in ipairs(children) do
            child.Parent = obj
        end
    end
    return obj
end

