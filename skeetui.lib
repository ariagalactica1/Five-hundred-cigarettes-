-- Helper function to create instance
local function create(className, props)
    local obj = Instance.new(className)
    for k,v in pairs(props) do
        if k == "Parent" then obj.Parent = v else obj[k] = v end
    end
    return obj
end

local function tween(obj, properties, duration)
    TweenService:Create(obj, TweenInfo.new(duration, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), properties):Play()
end

