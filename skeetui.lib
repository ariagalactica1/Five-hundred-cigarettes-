-- nav item builder
local function createNavItem(def)
    if def.type == "header" then
        local label = Create("TextLabel", {
            Parent = navList,
            Size = UDim2.new(1, 0, 0, 22),
            BackgroundTransparency = 1,
            Text = def.text,
            TextColor3 = Theme.SubTextColor,
            Font = Theme.Font,
            TextSize = 15,
            TextXAlignment = Enum.TextXAlignment.Left,
        })
        return label
    elseif def.type == "item" then
        local button = Create("TextButton", {
            Parent = navList,
            Size = UDim2.new(1, 0, 0, 26),
            BackgroundColor3 = Theme.DarkAccent,
            Text = def.text,
            TextColor3 = Theme.TextColor,
            Font = Theme.Font,
            TextSize = 16,
            AutoButtonColor = false,
            BorderSizePixel = 0,
        })
        roundify(button, Theme.CornerRadius)
        -- highlight on hover
        button.MouseEnter:Connect(function()
            TweenService:Create(button, TweenInfo.new(0.15), {BackgroundColor3 = Theme.AccentColor * 0.5}):Play()
        end)
        button.MouseLeave:Connect(function()
            if currentPage ~= def.text then
                TweenService:Create(button, TweenInfo.new(0.15), {BackgroundColor3 = Theme.DarkAccent}):Play()
            end
        end)
        -- page switching logic
        button.MouseButton1Click:Connect(function()
            -- highlight the selected item
            for _, navItem in pairs(navList:GetChildren()) do
                if navItem:IsA("TextButton") then
                    TweenService:Create(navItem, TweenInfo.new(0.15), {BackgroundColor3 = Theme.DarkAccent}):Play()
                end
            end
            TweenService:Create(button, TweenInfo.new(0.15), {BackgroundColor3 = Theme.AccentColor * 0.6}):Play()
            currentPage = def.text
            -- display corresponding page
            for pageName, pageFrame in pairs(pages) do
                pageFrame.Visible = (pageName == def.text)
            end
        end)
        return button
    end
end

-- create nav items
for _, def in ipairs(navigationDefinitions) do
    createNavItem(def)
end

