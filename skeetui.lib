-- helper to make a toggle control
local function createToggle(parent, labelText, default)
    local height = 24
    local holder = Create("Frame", {
        Parent = parent,
        Size = UDim2.new(1, 0, 0, height),
        BackgroundTransparency = 1,
    })
    local textLabel = Create("TextLabel", {
        Parent = holder,
        Position = UDim2.new(0, 0, 0, 0),
        Size = UDim2.new(1, -50, 1, 0),
        BackgroundTransparency = 1,
        Text = labelText,
        TextColor3 = Theme.TextColor,
        Font = Theme.Font,
        TextSize = 15,
        TextXAlignment = Enum.TextXAlignment.Left
    })
    -- toggle button background
    local toggleBG = Create("Frame", {
        Parent = holder,
        AnchorPoint = Vector2.new(1, 0.5),
        Position = UDim2.new(1, -8, 0.5, 0),
        Size = UDim2.new(0, Theme.ToggleWidth, 0, Theme.ToggleHeight),
        BackgroundColor3 = Theme.DarkAccent,
        BorderSizePixel = 0,
    })
    roundify(toggleBG, Theme.ToggleHeight/2)
    -- toggle knob
    local knob = Create("Frame", {
        Parent = toggleBG,
        Position = UDim2.new(default and 1 or 0, default and -Theme.ToggleHeight + 2 or 2, 0.5, 0),
        AnchorPoint = Vector2.new(0, 0.5),
        Size = UDim2.new(0, Theme.ToggleHeight - 4, 0, Theme.ToggleHeight - 4),
        BackgroundColor3 = default and Theme.AccentColor or Theme.SubTextColor,
        BorderSizePixel = 0,
    })
    roundify(knob, (Theme.ToggleHeight - 4)/2)

    local state = default or false
    local function setState(on)
        state = on
        local destX = on and (Theme.ToggleWidth - Theme.ToggleHeight + 2) or 2
        local destColor = on and Theme.AccentColor or Theme.SubTextColor
        TweenService:Create(knob, TweenInfo.new(0.15), {Position = UDim2.new(0, destX, 0.5, 0)}):Play()
        TweenService:Create(knob, TweenInfo.new(0.15), {BackgroundColor3 = destColor}):Play()
        -- lighten background when on
        TweenService:Create(toggleBG, TweenInfo.new(0.15), {BackgroundColor3 = on and Theme.AccentColor * 0.4 or Theme.DarkAccent}):Play()
    end
    toggleBG.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            setState(not state)
        end
    end)
    -- return state function for outside access
    return holder, function() return state end, setState
end

-- helper to create a slider control
local function createSlider(parent, labelText, minVal, maxVal, defaultVal)
    local height = 32
    local holder = Create("Frame", {
        Parent = parent,
        Size = UDim2.new(1, 0, 0, height),
        BackgroundTransparency = 1
    })
    local textLabel = Create("TextLabel", {
        Parent = holder,
        Position = UDim2.new(0, 0, 0, 0),
        Size = UDim2.new(1, 0, 0, 16),
        BackgroundTransparency = 1,
        Text = labelText,
        TextColor3 = Theme.TextColor,
        Font = Theme.Font,
        TextSize = 15,
        TextXAlignment = Enum.TextXAlignment.Left,
    })
    -- value display
    local valueLabel = Create("TextLabel", {
        Parent = holder,
        AnchorPoint = Vector2.new(1, 0),
        Position = UDim2.new(1, 0, 0, 0),
        Size = UDim2.new(0, 40, 0, 16),
        BackgroundTransparency = 1,
        Text = tostring(defaultVal),
        TextColor3 = Theme.SubTextColor,
        Font = Theme.Font,
        TextSize = 14,
        TextXAlignment = Enum.TextXAlignment.Right,
    })
    -- slider bar
    local bar = Create("Frame", {
        Parent = holder,
        Position = UDim2.new(0, 0, 0, 18),
        Size = UDim2.new(1, 0, 0, 6),
        BackgroundColor3 = Theme.DarkAccent,
        BorderSizePixel = 0,
    })
    roundify(bar, 3)
    -- fill inside bar
    local fill = Create("Frame", {
        Parent = bar,
        Size = UDim2.new((defaultVal - minVal)/(maxVal - minVal), 0, 1, 0),
        BackgroundColor3 = Theme.AccentColor,
        BorderSizePixel = 0,
    })
    roundify(fill, 3)
    -- knob
    local knob = Create("Frame", {
        Parent = bar,
        AnchorPoint = Vector2.new(0.5, 0.5),
        Position = UDim2.new((defaultVal - minVal)/(maxVal - minVal), 0, 0.5, 0),
        Size = UDim2.new(0, 12, 0, 12),
        BackgroundColor3 = Theme.AccentColor,
        BorderSizePixel = 0,
    })
    roundify(knob, 6)

    local current = defaultVal
    local dragging = false

    local function updateFromPosition(x)
        local absPos = bar.AbsolutePosition.X
        local width = bar.AbsoluteSize.X
        local percent = math.clamp((x - absPos) / width, 0, 1)
        current = math.floor((minVal + percent * (maxVal - minVal)) * 100) / 100
        fill.Size = UDim2.new(percent, 0, 1, 0)
        knob.Position = UDim2.new(percent, 0, 0.5, 0)
        valueLabel.Text = tostring(current)
    end

    bar.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
            updateFromPosition(input.Position.X)
        end
    end)
    knob.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
            updateFromPosition(input.Position.X)
        end
    end)
    UIS.InputChanged:Connect(function(input)
        if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
            updateFromPosition(input.Position.X)
        end
    end)
    UIS.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = false
        end
    end)
    return holder, function() return current end, function(v)
        current = math.clamp(v, minVal, maxVal)
        local percent = (current - minVal)/(maxVal - minVal)
        fill.Size = UDim2.new(percent, 0, 1, 0)
        knob.Position = UDim2.new(percent, 0, 0.5, 0)
        valueLabel.Text = tostring(current)
    end
end

-- helper to create dropdown control
local function createDropdown(parent, labelText, options, defaultIndex)
    local height = 32
    local holder = Create("Frame", {
        Parent = parent,
        Size = UDim2.new(1, 0, 0, height),
        BackgroundTransparency = 1,
        ClipsDescendants = true,
    })
    local textLabel = Create("TextLabel", {
        Parent = holder,
        Position = UDim2.new(0, 0, 0, 0),
        Size = UDim2.new(1, -100, 0, 16),
        BackgroundTransparency = 1,
        Text = labelText,
        TextColor3 = Theme.TextColor,
        Font = Theme.Font,
        TextSize = 15,
        TextXAlignment = Enum.TextXAlignment.Left,
    })
    -- drop button to show selection
    local dropButton = Create("TextButton", {
        Parent = holder,
        AnchorPoint = Vector2.new(1, 0),
        Position = UDim2.new(1, 0, 0, 0),
        Size = UDim2.new(0, 100, 0, 24),
        BackgroundColor3 = Theme.DarkAccent,
        BorderSizePixel = 0,
        AutoButtonColor = false,
        TextColor3 = Theme.TextColor,
        Font = Theme.Font,
        TextSize = 14,
    })
    roundify(dropButton, Theme.CornerRadius)
    local selected = defaultIndex or 1
    dropButton.Text = options[selected]
    local listHolder = Create("Frame", {
        Parent = holder,
        Position = UDim2.new(0, 0, 0, 24),
        Size = UDim2.new(1, 0, 0, (#options) * 22),
        BackgroundColor3 = Theme.PanelColor,
        BorderSizePixel = 0,
        Visible = false,
        ZIndex = 4,
    })
    roundify(listHolder, Theme.CornerRadius)
    local listLayout = Create("UIListLayout", {
        Parent = listHolder,
        SortOrder = Enum.SortOrder.LayoutOrder,
    })
    for i, opt in ipairs(options) do
        local btn = Create("TextButton", {
            Parent = listHolder,
            Size = UDim2.new(1, 0, 0, 22),
            BackgroundTransparency = 1,
            BorderSizePixel = 0,
            Text = opt,
            TextColor3 = Theme.TextColor,
            Font = Theme.Font,
            TextSize = 14,
            AutoButtonColor = false,
        })
        btn.MouseButton1Click:Connect(function()
            selected = i
            dropButton.Text = opt
            listHolder.Visible = false
        end)
        btn.MouseEnter:Connect(function()
            btn.BackgroundTransparency = 0.8
        end)
        btn.MouseLeave:Connect(function()
            btn.BackgroundTransparency = 1
        end)
    end
    -- open/close the list on click
    dropButton.MouseButton1Click:Connect(function()
        listHolder.Visible = not listHolder.Visible
    end)
    -- hide list when clicking outside
    UIS.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            if listHolder.Visible and not dropButton:IsAncestorOf(input.Target) and not listHolder:IsAncestorOf(input.Target) then
                listHolder.Visible = false
            end
        end
    end)
    return holder, function() return options[selected] end, function(newVal)
        for i, opt in ipairs(options) do
            if opt == newVal then
                selected = i
                dropButton.Text = opt
                return
            end
        end
    end
end

