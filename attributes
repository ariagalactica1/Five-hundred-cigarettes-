-- Roblox LocalPlayer Attribute Scanner - All Common Locations
-- Scans: Character Attributes, Player Attributes, Data folder, and Leaderstats
-- Place this in a LocalScript in StarterPlayer > StarterPlayerScripts

local Players = game:GetService("Players")
local player = Players.LocalPlayer

print("\nâ•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—")
print("â•‘  LOCALPLAYER ATTRIBUTE SCANNER - ALL LOCATIONS â•‘")
print("â•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•")
print("ğŸ‘¤ Player:", player.Name)
print("ğŸ†” UserId:", player.UserId)
print("\n")

local totalFound = 0

-- Helper function to safely get a value
local function safeGet(instance, key)
	if not instance then return nil end
	local success, result = pcall(function()
		return instance:GetAttribute(key) or (instance:FindFirstChild(key) and instance[key].Value)
	end)
	return success and result or nil
end

-- Function to print attributes from an instance
local function printAttributes(instance, label)
	if not instance then
		print("âŒ " .. label .. ": NOT FOUND")
		return
	end
	
	print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")
	print("ğŸ“ " .. label)
	print("   Instance: " .. instance.Name .. " (" .. instance.ClassName .. ")")
	
	local attributes = instance:GetAttributes()
	local hasAttributes = next(attributes) ~= nil
	
	if hasAttributes then
		print("   âœ¨ Attributes:")
		for name, value in pairs(attributes) do
			print(string.format("      â€¢ %s = %s (%s)", name, tostring(value), typeof(value)))
			totalFound = totalFound + 1
		end
	else
		print("   âš ï¸  No attributes found")
	end
end

-- Function to print values from a folder (like leaderstats/Data)
local function printFolderValues(folder, label)
	if not folder then
		print("âŒ " .. label .. ": NOT FOUND")
		return
	end
	
	print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")
	print("ğŸ“ " .. label)
	print("   Instance: " .. folder.Name .. " (" .. folder.ClassName .. ")")
	
	local children = folder:GetChildren()
	if #children > 0 then
		print("   ğŸ“Š Values:")
		for _, child in ipairs(children) do
			if child:IsA("ValueBase") then
				print(string.format("      â€¢ %s = %s (%s)", child.Name, tostring(child.Value), child.ClassName))
				totalFound = totalFound + 1
			end
		end
		
		-- Also check for attributes on the folder itself
		local attributes = folder:GetAttributes()
		if next(attributes) then
			print("   âœ¨ Attributes on folder:")
			for name, value in pairs(attributes) do
				print(string.format("      â€¢ %s = %s (%s)", name, tostring(value), typeof(value)))
				totalFound = totalFound + 1
			end
		end
	else
		print("   âš ï¸  No values found")
	end
end

-- Wait for character if it doesn't exist yet
if not player.Character then
	print("â³ Waiting for character to load...")
	player.CharacterAdded:Wait()
end

local character = player.Character

-- 1. Scan Character Attributes (getCharAttr)
print("\nğŸ” METHOD 1: Character Attributes (getCharAttr)")
printAttributes(character, "Character")

-- 2. Scan Player Attributes (getPlayerAttr)
print("\nğŸ” METHOD 2: Player Attributes (getPlayerAttr)")
printAttributes(player, "LocalPlayer")

-- 3. Scan Data Folder (getDataValue)
print("\nğŸ” METHOD 3: Data Folder (getDataValue)")
local dataFolder = player:FindFirstChild("Data")
printFolderValues(dataFolder, "Data Folder")

-- 4. Scan Leaderstats (getLeaderstats)
print("\nğŸ” METHOD 4: Leaderstats (getLeaderstats)")
local leaderstats = player:FindFirstChild("leaderstats")
printFolderValues(leaderstats, "Leaderstats Folder")

-- Additional common locations
print("\nğŸ” BONUS: Other Common Locations")

-- Check PlayerGui for attributes
local playerGui = player:FindFirstChild("PlayerGui")
printAttributes(playerGui, "PlayerGui")

-- Check Backpack
local backpack = player:FindFirstChild("Backpack")
printAttributes(backpack, "Backpack")

-- Check for common value containers
local stats = player:FindFirstChild("Stats")
if stats then
	printFolderValues(stats, "Stats Folder")
end

local status = character:FindFirstChild("Status")
if status then
	printFolderValues(status, "Character Status Folder")
end

-- Specific search for "Exposure" everywhere
print("\nâ”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")
print("ğŸ¯ SPECIFIC SEARCH: 'Exposure' Values")
print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")

local exposureFound = false

-- Check all possible locations
local searchLocations = {
	{instance = character, label = "Character Attribute"},
	{instance = player, label = "Player Attribute"},
	{instance = dataFolder, label = "Data Folder"},
	{instance = leaderstats, label = "Leaderstats"},
	{instance = stats, label = "Stats Folder"},
	{instance = status, label = "Status Folder"}
}

for _, location in ipairs(searchLocations) do
	if location.instance then
		-- Check as attribute
		local attrValue = location.instance:GetAttribute("Exposure")
		if attrValue ~= nil then
			print(string.format("âœ… Found in %s: Exposure = %s (%s)", location.label, tostring(attrValue), typeof(attrValue)))
			exposureFound = true
		end
		
		-- Check as child value object
		local valueObj = location.instance:FindFirstChild("Exposure")
		if valueObj and valueObj:IsA("ValueBase") then
			print(string.format("âœ… Found in %s: Exposure = %s (%s)", location.label, tostring(valueObj.Value), valueObj.ClassName))
			exposureFound = true
		end
	end
end

if not exposureFound then
	print("âŒ 'Exposure' not found in any common location")
end

print("\nâ”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”")
print("âœ… SCAN COMPLETE")
print("ğŸ“Š Total Values/Attributes Found:", totalFound)
print("â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”â”\n")
