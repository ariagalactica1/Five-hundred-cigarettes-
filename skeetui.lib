-- Toggle Function
local function createToggle(labelText, stateKey)
    local container = create("Frame",{Parent=scrollFrame, Size=UDim2.new(1,-40,0,30), BackgroundTransparency=1})
    create("TextLabel",{Parent=container, Text=labelText, Font=Enum.Font.Gotham, TextSize=14, TextColor3=COLORS.TextSecondary, BackgroundTransparency=1, Position=UDim2.new(0,0,0,0), Size=UDim2.new(0.7,0,1,0), TextXAlignment=Enum.TextXAlignment.Left})
    local button = create("TextButton",{Parent=container, Size=UDim2.new(0,36,0,18), Position=UDim2.new(1,-36,0.5,-9), BackgroundColor3=UI_STATE[stateKey] and COLORS.Primary or COLORS.ControlBG, BorderSizePixel=0, Text=""})
    local knob = create("Frame",{Parent=button, Size=UDim2.new(0,16,0,16), Position=UI_STATE[stateKey] and UDim2.new(1,-16,0,0) or UDim2.new(0,0,0,0), BackgroundColor3=Color3.fromRGB(255,255,255), BorderSizePixel=0})
    create("UICorner",{Parent=knob})
    create("UICorner",{Parent=button})
    button.MouseButton1Click:Connect(function()
        UI_STATE[stateKey] = not UI_STATE[stateKey]
        tween(button,{BackgroundColor3=UI_STATE[stateKey] and COLORS.Primary or COLORS.ControlBG},0.2)
        tween(knob,{Position=UI_STATE[stateKey] and UDim2.new(1,-16,0,0) or UDim2.new(0,0,0,0)},0.2)
    end)
    return container
end

-- Slider Function
local function createSlider(labelText,stateKey,maxValue)
    local container = create("Frame",{Parent=scrollFrame, Size=UDim2.new(1,-40,0,30), BackgroundTransparency=1})
    create("TextLabel",{Parent=container, Text=labelText, Font=Enum.Font.Gotham, TextSize=14, TextColor3=COLORS.TextSecondary, BackgroundTransparency=1, Position=UDim2.new(0,0,0,0), Size=UDim2.new(0.3,0,1,0), TextXAlignment=Enum.TextXAlignment.Left})
    local sliderTrack = create("Frame",{Parent=container, Size=UDim2.new(0.6,0,0.4,0), Position=UDim2.new(0.35,0,0.3,0), BackgroundColor3=COLORS.ControlBG, BorderSizePixel=0})
    local sliderFill = create("Frame",{Parent=sliderTrack, Size=UDim2.new(UI_STATE[stateKey]/maxValue,0,1,0), BackgroundColor3=COLORS.Primary, BorderSizePixel=0})
    local thumb = create("Frame",{Parent=sliderTrack, Size=UDim2.new(0,12,1,0), BackgroundColor3=Color3.fromRGB(255,255,255), BorderSizePixel=0})
    create("UICorner",{Parent=sliderTrack})
    create("UICorner",{Parent=sliderFill})
    create("UICorner",{Parent=thumb})

    local dragging = false
    thumb.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then dragging = true end
    end)
    thumb.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then dragging = false end
    end)
    UserInputService.InputChanged:Connect(function(input)
        if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
            local relative = math.clamp((input.Position.X - sliderTrack.AbsolutePosition.X)/sliderTrack.AbsoluteSize.X,0,1)
            UI_STATE[stateKey] = math.floor(relative*maxValue)
            sliderFill.Size = UDim2.new(relative,0,1,0)
            thumb.Position = UDim2.new(relative,0,0,0)
        end
    end)
    return container
end

