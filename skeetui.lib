	bgMain      = hex("101110"),
	bgSidebar   = hex("1c1d1c"),
	bgActiveNav = hex("2c2d2c"),
	bgControl   = hex("252525"),
	border      = hex("3a3a3a"),
	accent      = hex("9efd38"),
	textWhite   = hex("ffffff"),
	textLight   = hex("cccccc"),
	textMuted   = hex("6c6c6c"),
