-- NAV ITEMS
local function createNavItem(iconText, labelText)
    local item = create("Frame",{Parent=sidebar, Size=UDim2.new(1,0,0,36), BackgroundColor3=COLORS.SidebarBG, BorderSizePixel=0})
    local icon = create("TextLabel",{Parent=item, Size=UDim2.new(0,24,1,0), BackgroundTransparency=1, Text=iconText, Font=Enum.Font.GothamBold, TextSize=18, TextColor3=COLORS.Primary})
    local label = create("TextLabel",{Parent=item, Size=UDim2.new(1,-30,1,0), Position=UDim2.new(0,30,0,0), BackgroundTransparency=1, Text=labelText, Font=Enum.Font.Gotham, TextSize=16, TextColor3=COLORS.TextPrimary, TextXAlignment=Enum.TextXAlignment.Left})
    item.MouseEnter:Connect(function() tween(item,{BackgroundColor3=COLORS.Hover},0.2) end)
    item.MouseLeave:Connect(function() tween(item,{BackgroundColor3=COLORS.SidebarBG},0.2) end)
    return item
end

