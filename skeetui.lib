--[[
    GAMESENSE UI LIBRARY - PERFECT RECREATION
    
    HOW TO USE:
    
    local Library = loadstring(game:HttpGet("your_script_here"))()
    
    -- Create window
    local Window = Library:CreateWindow("GAMESENSE")
    
    -- Create tabs
    local Tab1 = Window:CreateTab("RageBot", "üéØ")
    local Tab2 = Window:CreateTab("Anti Aim", "üõ°Ô∏è", true) -- true = selected by default
    
    -- Add elements to tabs
    Tab2:AddSection("Anti-aimbot angles")
    Tab2:AddToggle("Enabled", true, function(state)
        print("Enabled:", state)
    end)
    
    Tab2:AddDropdown("Yaw base", "Local view", {"Local view", "At target", "Freestanding"}, function(option)
        print("Selected:", option)
    end)
    
    Tab2:AddSlider("Yaw", 0, -180, 180, function(value)
        print("Yaw:", value)
    end)
    
    Tab2:AddSection("Fake lag", true) -- true = right column
    Tab2:AddToggle("Enabled", true, function(state) end, true) -- true = right column
]]

local Library = {}
local UserInputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")
local Players = game:GetService("Players")

-- PRECISE COLORS FROM IMAGE
local Colors = {
    Background = Color3.fromRGB(17, 17, 17),
    Sidebar = Color3.fromRGB(12, 12, 12),
    TopBar = Color3.fromRGB(12, 12, 12),
    Panel = Color3.fromRGB(22, 22, 22),
    PanelHover = Color3.fromRGB(28, 28, 28),
    Accent = Color3.fromRGB(155, 195, 65), -- That perfect lime green
    Text = Color3.fromRGB(210, 210, 210),
    TextDark = Color3.fromRGB(140, 140, 140),
    TextDarker = Color3.fromRGB(100, 100, 100),
    SliderFill = Color3.fromRGB(155, 195, 65),
    ToggleEnabled = Color3.fromRGB(155, 195, 65),
    Border = Color3.fromRGB(35, 35, 35)
}

-- Transparency values for that authentic look
local Transparency = {
    Background = 0.15,
    Sidebar = 0.05,
    TopBar = 0.05,
    Panel = 0
}

function Library:CreateWindow(title)
    local Window = {}
    Window.Tabs = {}
    Window.CurrentTab = nil
    
    -- Create ScreenGui
    local ScreenGui = Instance.new("ScreenGui")
    ScreenGui.Name = "GameSenseUI"
    ScreenGui.ResetOnSpawn = false
    ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    ScreenGui.Parent = Players.LocalPlayer:WaitForChild("PlayerGui")
    
    -- Main container with transparency
    local Main = Instance.new("Frame")
    Main.Name = "Main"
    Main.Size = UDim2.new(0, 690, 0, 485)
    Main.Position = UDim2.new(0.5, -345, 0.5, -242)
    Main.BackgroundColor3 = Colors.Background
    Main.BackgroundTransparency = Transparency.Background
    Main.BorderSizePixel = 0
    Main.ClipsDescendants = true
    Main.Parent = ScreenGui
    
    -- Subtle outer glow/shadow
    local Shadow = Instance.new("ImageLabel")
    Shadow.Name = "Shadow"
    Shadow.BackgroundTransparency = 1
    Shadow.Position = UDim2.new(0, -15, 0, -15)
    Shadow.Size = UDim2.new(1, 30, 1, 30)
    Shadow.ZIndex = 0
    Shadow.Image = "rbxasset://textures/ui/GuiImagePlaceholder.png"
    Shadow.ImageColor3 = Color3.fromRGB(0, 0, 0)
    Shadow.ImageTransparency = 0.7
    Shadow.ScaleType = Enum.ScaleType.Slice
    Shadow.SliceCenter = Rect.new(10, 10, 10, 10)
    Shadow.Parent = Main
    
    -- TOP BAR
    local TopBar = Instance.new("Frame")
    TopBar.Name = "TopBar"
    TopBar.Size = UDim2.new(1, 0, 0, 32)
    TopBar.BackgroundColor3 = Colors.TopBar
    TopBar.BackgroundTransparency = Transparency.TopBar
    TopBar.BorderSizePixel = 0
    TopBar.Parent = Main
    
    local TopBarBorder = Instance.new("Frame")
    TopBarBorder.Size = UDim2.new(1, 0, 0, 1)
    TopBarBorder.Position = UDim2.new(0, 0, 1, 0)
    TopBarBorder.BackgroundColor3 = Colors.Border
    TopBarBorder.BorderSizePixel = 0
    TopBarBorder.Parent = TopBar
    
    local Title = Instance.new("TextLabel")
    Title.Size = UDim2.new(0, 150, 1, 0)
    Title.Position = UDim2.new(0, 12, 0, 0)
    Title.BackgroundTransparency = 1
    Title.Text = title
    Title.TextColor3 = Colors.Text
    Title.TextSize = 12
    Title.Font = Enum.Font.GothamBold
    Title.TextXAlignment = Enum.TextXAlignment.Left
    Title.Parent = TopBar
    
    -- Top bar icons (right side)
    local function createTopIcon(icon, pos)
        local btn = Instance.new("TextButton")
        btn.Size = UDim2.new(0, 28, 0, 28)
        btn.Position = UDim2.new(1, -32 * pos - 4, 0, 2)
        btn.BackgroundTransparency = 1
        btn.Text = icon
        btn.TextColor3 = Colors.TextDark
        btn.TextSize = 14
        btn.Font = Enum.Font.GothamBold
        btn.Parent = TopBar
        return btn
    end
    
    createTopIcon("‚öô", 3)
    createTopIcon("‚Üª", 2)
    createTopIcon("üîç", 1)
    
    -- SIDEBAR
    local Sidebar = Instance.new("Frame")
    Sidebar.Name = "Sidebar"
    Sidebar.Size = UDim2.new(0, 105, 1, -32)
    Sidebar.Position = UDim2.new(0, 0, 0, 32)
    Sidebar.BackgroundColor3 = Colors.Sidebar
    Sidebar.BackgroundTransparency = Transparency.Sidebar
    Sidebar.BorderSizePixel = 0
    Sidebar.Parent = Main
    
    local SidebarBorder = Instance.new("Frame")
    SidebarBorder.Size = UDim2.new(0, 1, 1, 0)
    SidebarBorder.Position = UDim2.new(1, 0, 0, 0)
    SidebarBorder.BackgroundColor3 = Colors.Border
    SidebarBorder.BorderSizePixel = 0
    SidebarBorder.Parent = Sidebar
    
    -- Sidebar buttons container
    local TabContainer = Instance.new("Frame")
    TabContainer.Size = UDim2.new(1, 0, 1, -40)
    TabContainer.Position = UDim2.new(0, 0, 0, 0)
    TabContainer.BackgroundTransparency = 1
    TabContainer.Parent = Sidebar
    
    -- USER INFO (Bottom of sidebar)
    local UserInfo = Instance.new("Frame")
    UserInfo.Size = UDim2.new(1, 0, 0, 40)
    UserInfo.Position = UDim2.new(0, 0, 1, -40)
    UserInfo.BackgroundTransparency = 1
    UserInfo.Parent = Sidebar
    
    local UserAvatar = Instance.new("ImageLabel")
    UserAvatar.Size = UDim2.new(0, 24, 0, 24)
    UserAvatar.Position = UDim2.new(0, 8, 0, 8)
    UserAvatar.BackgroundColor3 = Colors.Panel
    UserAvatar.BorderSizePixel = 0
    UserAvatar.Image = Players:GetUserThumbnailAsync(Players.LocalPlayer.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size48x48)
    UserAvatar.Parent = UserInfo
    
    local UserAvatarCorner = Instance.new("UICorner")
    UserAvatarCorner.CornerRadius = UDim.new(1, 0)
    UserAvatarCorner.Parent = UserAvatar
    
    local UserName = Instance.new("TextLabel")
    UserName.Size = UDim2.new(1, -40, 0, 12)
    UserName.Position = UDim2.new(0, 36, 0, 8)
    UserName.BackgroundTransparency = 1
    UserName.Text = Players.LocalPlayer.Name
    UserName.TextColor3 = Colors.Text
    UserName.TextSize = 10
    UserName.Font = Enum.Font.Gotham
    UserName.TextXAlignment = Enum.TextXAlignment.Left
    UserName.TextTruncate = Enum.TextTruncate.AtEnd
    UserName.Parent = UserInfo
    
    local UserStats = Instance.new("TextLabel")
    UserStats.Size = UDim2.new(1, -40, 0, 10)
    UserStats.Position = UDim2.new(0, 36, 0, 20)
    UserStats.BackgroundTransparency = 1
    UserStats.Text = "77 HP / 24 SV"
    UserStats.TextColor3 = Colors.Accent
    UserStats.TextSize = 8
    UserStats.Font = Enum.Font.Gotham
    UserStats.TextXAlignment = Enum.TextXAlignment.Left
    UserStats.Parent = UserInfo
    
    -- CONTENT AREA
    local ContentContainer = Instance.new("Frame")
    ContentContainer.Name = "Content"
    ContentContainer.Size = UDim2.new(1, -105, 1, -32)
    ContentContainer.Position = UDim2.new(0, 105, 0, 32)
    ContentContainer.BackgroundTransparency = 1
    ContentContainer.ClipsDescendants = true
    ContentContainer.Parent = Main
    
    -- Make draggable
    local dragging, dragInput, dragStart, startPos
    
    TopBar.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
            dragStart = input.Position
            startPos = Main.Position
        end
    end)
    
    TopBar.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = false
        end
    end)
    
    UserInputService.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement then
            dragInput = input
            if dragging then
                local delta = input.Position - dragStart
                Main.Position = UDim2.new(
                    startPos.X.Scale, startPos.X.Offset + delta.X,
                    startPos.Y.Scale, startPos.Y.Offset + delta.Y
                )
            end
        end
    end)
    
    -- TAB CREATION
    function Window:CreateTab(name, icon, selected)
        local Tab = {}
        Tab.Name = name
        Tab.Elements = {Left = {}, Right = {}}
        Tab.LeftY = 12
        Tab.RightY = 12
        
        -- Tab button
        local TabButton = Instance.new("TextButton")
        TabButton.Name = name
        TabButton.Size = UDim2.new(1, -8, 0, 26)
        TabButton.Position = UDim2.new(0, 4, 0, (#Window.Tabs) * 28 + 4)
        TabButton.BackgroundColor3 = selected and Colors.Panel or Color3.fromRGB(0, 0, 0)
        TabButton.BackgroundTransparency = selected and 0 or 1
        TabButton.BorderSizePixel = 0
        TabButton.AutoButtonColor = false
        TabButton.Text = ""
        TabButton.Parent = TabContainer
        
        if selected then
            local corner = Instance.new("UICorner")
            corner.CornerRadius = UDim.new(0, 2)
            corner.Parent = TabButton
        end
        
        local TabLabel = Instance.new("TextLabel")
        TabLabel.Size = UDim2.new(1, -8, 1, 0)
        TabLabel.Position = UDim2.new(0, 8, 0, 0)
        TabLabel.BackgroundTransparency = 1
        TabLabel.Text = name
        TabLabel.TextColor3 = selected and Colors.Accent or Colors.TextDark
        TabLabel.TextSize = 11
        TabLabel.Font = Enum.Font.Gotham
        TabLabel.TextXAlignment = Enum.TextXAlignment.Left
        TabLabel.Parent = TabButton
        
        -- Selection indicator
        local Indicator = Instance.new("Frame")
        Indicator.Size = UDim2.new(0, 2, 1, 0)
        Indicator.Position = UDim2.new(0, 0, 0, 0)
        Indicator.BackgroundColor3 = Colors.Accent
        Indicator.BorderSizePixel = 0
        Indicator.Visible = selected or false
        Indicator.Parent = TabButton
        
        -- Tab content (two columns)
        local TabContent = Instance.new("Frame")
        TabContent.Name = name .. "Content"
        TabContent.Size = UDim2.new(1, 0, 1, 0)
        TabContent.BackgroundTransparency = 1
        TabContent.Visible = selected or false
        TabContent.Parent = ContentContainer
        
        local LeftColumn = Instance.new("ScrollingFrame")
        LeftColumn.Name = "Left"
        LeftColumn.Size = UDim2.new(0.5, -8, 1, 0)
        LeftColumn.Position = UDim2.new(0, 8, 0, 0)
        LeftColumn.BackgroundTransparency = 1
        LeftColumn.BorderSizePixel = 0
        LeftColumn.ScrollBarThickness = 4
        LeftColumn.ScrollBarImageColor3 = Colors.TextDarker
        LeftColumn.CanvasSize = UDim2.new(0, 0, 0, 0)
        LeftColumn.Parent = TabContent
        
        local RightColumn = Instance.new("ScrollingFrame")
        RightColumn.Name = "Right"
        RightColumn.Size = UDim2.new(0.5, -8, 1, 0)
        RightColumn.Position = UDim2.new(0.5, 4, 0, 0)
        RightColumn.BackgroundTransparency = 1
        RightColumn.BorderSizePixel = 0
        RightColumn.ScrollBarThickness = 4
        RightColumn.ScrollBarImageColor3 = Colors.TextDarker
        RightColumn.CanvasSize = UDim2.new(0, 0, 0, 0)
        RightColumn.Parent = TabContent
        
        Tab.LeftColumn = LeftColumn
        Tab.RightColumn = RightColumn
        
        -- Tab switching
        TabButton.MouseButton1Click:Connect(function()
            for _, tab in pairs(Window.Tabs) do
                tab.Button.BackgroundTransparency = 1
                tab.Label.TextColor3 = Colors.TextDark
                tab.Indicator.Visible = false
                tab.Content.Visible = false
                if tab.Button:FindFirstChild("UICorner") then
                    tab.Button.UICorner:Destroy()
                end
            end
            
            TabButton.BackgroundTransparency = 0
            TabButton.BackgroundColor3 = Colors.Panel
            TabLabel.TextColor3 = Colors.Accent
            Indicator.Visible = true
            TabContent.Visible = true
            
            local corner = Instance.new("UICorner")
            corner.CornerRadius = UDim.new(0, 2)
            corner.Parent = TabButton
        end)
        
        Tab.Button = TabButton
        Tab.Label = TabLabel
        Tab.Indicator = Indicator
        Tab.Content = TabContent
        
        -- SECTION
        function Tab:AddSection(text, rightSide)
            local column = rightSide and RightColumn or LeftColumn
            local yPos = rightSide and self.RightY or self.LeftY
            
            local Section = Instance.new("TextLabel")
            Section.Size = UDim2.new(1, -4, 0, 24)
            Section.Position = UDim2.new(0, 2, 0, yPos)
            Section.BackgroundTransparency = 1
            Section.Text = text
            Section.TextColor3 = Colors.Text
            Section.TextSize = 11
            Section.Font = Enum.Font.GothamBold
            Section.TextXAlignment = Enum.TextXAlignment.Left
            Section.TextYAlignment = Enum.TextYAlignment.Bottom
            Section.Parent = column
            
            if rightSide then
                self.RightY = self.RightY + 28
                RightColumn.CanvasSize = UDim2.new(0, 0, 0, self.RightY)
            else
                self.LeftY = self.LeftY + 28
                LeftColumn.CanvasSize = UDim2.new(0, 0, 0, self.LeftY)
            end
        end
        
        -- TOGGLE (Sleek design)
        function Tab:AddToggle(text, default, callback, rightSide)
            local column = rightSide and RightColumn or LeftColumn
            local yPos = rightSide and self.RightY or self.LeftY
            
            local state = default or false
            
            local Container = Instance.new("Frame")
            Container.Size = UDim2.new(1, -4, 0, 20)
            Container.Position = UDim2.new(0, 2, 0, yPos)
            Container.BackgroundTransparency = 1
            Container.Parent = column
            
            local Label = Instance.new("TextLabel")
            Label.Size = UDim2.new(1, -40, 1, 0)
            Label.BackgroundTransparency = 1
            Label.Text = text
            Label.TextColor3 = Colors.TextDark
            Label.TextSize = 10
            Label.Font = Enum.Font.Gotham
            Label.TextXAlignment = Enum.TextXAlignment.Left
            Label.Parent = Container
            
            local ToggleButton = Instance.new("TextButton")
            ToggleButton.Size = UDim2.new(0, 32, 0, 14)
            ToggleButton.Position = UDim2.new(1, -32, 0.5, -7)
            ToggleButton.BackgroundColor3 = state and Colors.ToggleEnabled or Colors.Panel
            ToggleButton.BorderSizePixel = 0
            ToggleButton.AutoButtonColor = false
            ToggleButton.Text = ""
            ToggleButton.Parent = Container
            
            local ToggleCorner = Instance.new("UICorner")
            ToggleCorner.CornerRadius = UDim.new(0, 7)
            ToggleCorner.Parent = ToggleButton
            
            local Knob = Instance.new("Frame")
            Knob.Size = UDim2.new(0, 10, 0, 10)
            Knob.Position = state and UDim2.new(1, -12, 0.5, -5) or UDim2.new(0, 2, 0.5, -5)
            Knob.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Knob.BorderSizePixel = 0
            Knob.Parent = ToggleButton
            
            local KnobCorner = Instance.new("UICorner")
            KnobCorner.CornerRadius = UDim.new(1, 0)
            KnobCorner.Parent = Knob
            
            ToggleButton.MouseButton1Click:Connect(function()
                state = not state
                
                TweenService:Create(ToggleButton, TweenInfo.new(0.15, Enum.EasingStyle.Quad), {
                    BackgroundColor3 = state and Colors.ToggleEnabled or Colors.Panel
                }):Play()
                
                TweenService:Create(Knob, TweenInfo.new(0.15, Enum.EasingStyle.Quad), {
                    Position = state and UDim2.new(1, -12, 0.5, -5) or UDim2.new(0, 2, 0.5, -5)
                }):Play()
                
                if callback then
                    callback(state)
                end
            end)
            
            if rightSide then
                self.RightY = self.RightY + 22
                RightColumn.CanvasSize = UDim2.new(0, 0, 0, self.RightY)
            else
                self.LeftY = self.LeftY + 22
                LeftColumn.CanvasSize = UDim2.new(0, 0, 0, self.LeftY)
            end
        end
        
        -- DROPDOWN (Sleek)
        function Tab:AddDropdown(text, default, options, callback, rightSide)
            local column = rightSide and RightColumn or RightColumn
            local yPos = rightSide and self.RightY or self.LeftY
            
            local selected = default or ""
            
            local Container = Instance.new("Frame")
            Container.Size = UDim2.new(1, -4, 0, 20)
            Container.Position = UDim2.new(0, 2, 0, yPos)
            Container.BackgroundTransparency = 1
            Container.Parent = column
            
            local Label = Instance.new("TextLabel")
            Label.Size = UDim2.new(0, 80, 1, 0)
            Label.BackgroundTransparency = 1
            Label.Text = text
            Label.TextColor3 = Colors.TextDark
            Label.TextSize = 10
            Label.Font = Enum.Font.Gotham
            Label.TextXAlignment = Enum.TextXAlignment.Left
            Label.Parent = Container
            
            local Dropdown = Instance.new("TextButton")
            Dropdown.Size = UDim2.new(1, -85, 0, 16)
            Dropdown.Position = UDim2.new(0, 82, 0.5, -8)
            Dropdown.BackgroundColor3 = Colors.Panel
            Dropdown.BorderSizePixel = 0
            Dropdown.AutoButtonColor = false
            Dropdown.Text = selected .. " ‚ñº"
            Dropdown.TextColor3 = Colors.Text
            Dropdown.TextSize = 9
            Dropdown.Font = Enum.Font.Gotham
            Dropdown.TextXAlignment = Enum.TextXAlignment.Center
            Dropdown.Parent = Container
            
            local DropCorner = Instance.new("UICorner")
            DropCorner.CornerRadius = UDim.new(0, 2)
            DropCorner.Parent = Dropdown
            
            if rightSide then
                self.RightY = self.RightY + 22
                RightColumn.CanvasSize = UDim2.new(0, 0, 0, self.RightY)
            else
                self.LeftY = self.LeftY + 22
                LeftColumn.CanvasSize = UDim2.new(0, 0, 0, self.LeftY)
            end
        end
        
        -- SLIDER (Thin and precise)
        function Tab:AddSlider(text, default, min, max, callback, rightSide)
            local column = rightSide and RightColumn or LeftColumn
            local yPos = rightSide and self.RightY or self.LeftY
            
            local value = default or 0
            
            local Container = Instance.new("Frame")
            Container.Size = UDim2.new(1, -4, 0, 32)
            Container.Position = UDim2.new(0, 2, 0, yPos)
            Container.BackgroundTransparency = 1
            Container.Parent = column
            
            local Label = Instance.new("TextLabel")
            Label.Size = UDim2.new(1, 0, 0, 14)
            Label.BackgroundTransparency = 1
            Label.Text = text
            Label.TextColor3 = Colors.TextDark
            Label.TextSize = 10
            Label.Font = Enum.Font.Gotham
            Label.TextXAlignment = Enum.TextXAlignment.Left
            Label.Parent = Container
            
            local SliderBack = Instance.new("Frame")
            SliderBack.Size = UDim2.new(1, -4, 0, 3)
            SliderBack.Position = UDim2.new(0, 2, 0, 18)
            SliderBack.BackgroundColor3 = Colors.Panel
            SliderBack.BorderSizePixel = 0
            SliderBack.Parent = Container
            
            local SliderFill = Instance.new("Frame")
            SliderFill.Size = UDim2.new(0.5, 0, 1, 0)
            SliderFill.BackgroundColor3 = Colors.SliderFill
            SliderFill.BorderSizePixel = 0
            SliderFill.Parent = SliderBack
            
            local SliderKnob = Instance.new("Frame")
            SliderKnob.Size = UDim2.new(0, 8, 0, 8)
            SliderKnob.Position = UDim2.new(0.5, -4, 0.5, -4)
            SliderKnob.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            SliderKnob.BorderSizePixel = 0
            SliderKnob.Parent = SliderBack
            
            local KnobCorner = Instance.new("UICorner")
            KnobCorner.CornerRadius = UDim.new(1, 0)
            KnobCorner.Parent = SliderKnob
            
            local dragging = false
            
            SliderKnob.InputBegan:Connect(function(input)
                if input.UserInputType == Enum.UserInputType.MouseButton1 then
                    dragging = true
                end
            end)
            
            UserInputService.InputEnded:Connect(function(input)
                if input.UserInputType == Enum.UserInputType.MouseButton1 then
                    dragging = false
                end
            end)
            
            UserInputService.InputChanged:Connect(function(input)
                if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
                    local pos = math.clamp((input.Position.X - SliderBack.AbsolutePosition.X) / SliderBack.AbsoluteSize.X, 0, 1)
                    SliderFill.Size = UDim2.new(pos, 0, 1, 0)
                    SliderKnob.Position = UDim2.new(pos, -4, 0.5, -4)
                    value = min + (max - min) * pos
                    if callback then
                        callback(math.floor(value))
                    end
                end
            end)
            
            if rightSide then
                self.RightY = self.RightY + 34
                RightColumn.CanvasSize = UDim2.new(0, 0, 0, self.RightY)
            else
                self.LeftY = self.LeftY + 34
                LeftColumn.CanvasSize = UDim2.new(0, 0, 0, self.LeftY)
            end
        end
        
        table.insert(Window.Tabs, Tab)
        return Tab
    end
    
    return Window
end

