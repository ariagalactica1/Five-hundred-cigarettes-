		SetText = function(t) lbl.Text = t end
	}
end

-- Slider (0..100)
local function mkSlider(parent, defaultValue, onChanged)
	local holder = Instance.new("Frame")
	holder.BackgroundTransparency = 1
	holder.Size = UDim2.new(1, 0, 0, 28)
	holder.Parent = parent

	local track = Instance.new("Frame")
	track.BackgroundColor3 = Theme.bgControl
	track.BorderSizePixel = 0
	track.Size = UDim2.new(1, -40, 0, 8)
	track.AnchorPoint = Vector2.new(0, 0.5)
	track.Position = UDim2.new(0, 0, 0.5, 0)
	track.Parent = holder
	corner(track, 5)
	stroke(track, Theme.border, 1)

	local fill = Instance.new("Frame")
	fill.BackgroundColor3 = Theme.accent
	fill.BorderSizePixel = 0
	fill.Size = UDim2.new(0, 0, 1, 0)
	fill.Parent = track
	corner(fill, 5)

	local knob = Instance.new("Frame")
	knob.BackgroundColor3 = Theme.accent
	knob.BorderSizePixel = 0
	knob.Size = UDim2.fromOffset(16, 16)
	knob.AnchorPoint = Vector2.new(0.5, 0.5)
	knob.Position = UDim2.new(0, 0, 0.5, 0)
	knob.Parent = track
	corner(knob, 8)

	local valLbl = mkLabel(holder, tostring(defaultValue or 0), 14, Theme.textMuted, false, false)
	valLbl.Size = UDim2.fromOffset(36, 28)
	valLbl.AnchorPoint = Vector2.new(1, 0)
	valLbl.Position = UDim2.new(1, 0, 0, 0)
	valLbl.TextXAlignment = Enum.TextXAlignment.Right

	local dragging = false
	local value = math.clamp(tonumber(defaultValue or 0) or 0, 0, 100)

	local function apply(v)
		value = math.clamp(v, 0, 100)
		local ratio = (track.AbsoluteSize.X > 0) and (value / 100) or 0
		fill.Size = UDim2.new(ratio, 0, 1, 0)
		knob.Position = UDim2.new(ratio, 0, 0.5, 0)
		valLbl.Text = tostring(math.floor(value + 0.5))
		if onChanged then
			onChanged(value)
		end
	end

	task.defer(function() apply(value) end)

	local function setFromX(x)
		local rel = x - track.AbsolutePosition.X
		local ratio = rel / math.max(track.AbsoluteSize.X, 1)
		apply(ratio * 100)
	end

	track.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			setFromX(input.Position.X)
		end
	end)

	UserInputService.InputChanged:Connect(function(input)
		if dragging and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
			setFromX(input.Position.X)
		end
	end)

	UserInputService.InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = false
		end
	end)

	return {
		Instance = holder,
		Get = function() return value end,
		Set = function(v) apply(v) end,
		Label = valLbl,
	}
end

