local function addNav(text, active)
	local btn = Instance.new("TextButton")
	btn.BackgroundColor3 = active and Theme.bgActiveNav or Color3.new(0,0,0)
	btn.BackgroundTransparency = active and 0 or 1
	btn.AutoButtonColor = true
	btn.Text = text
	btn.TextXAlignment = Enum.TextXAlignment.Left
	btn.Font = Enum.Font.Gotham
	btn.TextSize = 14
	btn.TextColor3 = active and Theme.textWhite or Theme.textLight
	btn.Size = UDim2.new(1, 0, 0, 32)
	btn.BorderSizePixel = 0
	corner(btn, 6)
	btn.Parent = Nav

	if active then
		local accent = Instance.new("Frame")
		accent.Size = UDim2.new(0, 3, 0, 20)
		accent.Position = UDim2.new(0, 0, 0.5, -10)
		accent.BackgroundColor3 = Theme.accent
		accent.BorderSizePixel = 0
		accent.Parent = btn
	end

	return btn
end

