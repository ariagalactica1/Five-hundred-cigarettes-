-- Toggle control
local function mkToggle(parent, defaultOn)
	local holder = Instance.new("Frame")
	holder.BackgroundTransparency = 1
	holder.Size = UDim2.fromOffset(38, 22)
	holder.Parent = parent

	local track = Instance.new("TextButton")
	track.AutoButtonColor = false
	track.BackgroundColor3 = Theme.bgControl
	track.Text = ""
	track.BorderSizePixel = 0
	track.Size = UDim2.fromScale(1,1)
	track.Parent = holder
	corner(track, 22)
	stroke(track, Theme.border, 1)

	local knob = Instance.new("Frame")
	knob.Size = UDim2.fromOffset(16, 16)
	knob.Position = UDim2.fromOffset(2, 3)
	knob.BackgroundColor3 = Color3.fromRGB(136,136,136)
	knob.BorderSizePixel = 0
	knob.Parent = track
	corner(knob, 8)

	local state = defaultOn and true or false

	local function apply()
		if state then
			track.BackgroundColor3 = Theme.accent
			stroke(track, Theme.accent, 1)
			knob.BackgroundColor3 = Theme.bgSidebar
			knob.Position = UDim2.fromOffset(20, 3)
		else
			track.BackgroundColor3 = Theme.bgControl
			stroke(track, Theme.border, 1)
			knob.BackgroundColor3 = Color3.fromRGB(136,136,136)
			knob.Position = UDim2.fromOffset(2, 3)
		end
	end
	apply()

	track.MouseButton1Click:Connect(function()
		state = not state
		apply()
	end)

	return {
		Instance = holder,
		Get = function() return state end,
		Set = function(v) state = v; apply() end,
	}
end

